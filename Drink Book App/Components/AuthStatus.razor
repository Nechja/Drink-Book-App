@using Microsoft.AspNetCore.Components.Authorization
@using Microsoft.AspNetCore.Authentication

<MudNavMenu>
<AuthorizeView>
    
    <Authorized>
        <MudNavLink Href="logout">
            <MudIcon Icon="@Icons.Material.Filled.Logout"></MudIcon>Log out
        </MudNavLink>
    </Authorized>
    <NotAuthorized>
        <MudNavLink Href="/login?redirectUri=/">
            <MudIcon Icon="@Icons.Material.Filled.Login"></MudIcon>Log in
        </MudNavLink>
        
    </NotAuthorized>
</AuthorizeView>
</MudNavMenu>
